{
  "name": "javascript-utils",
  "version": "1.0.0",
  "description": "JavaScript 常用函数工具库。",
  "main": "dist/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "npm run lint & webpack --env development & live-server --no-browser",
    "build": "npm run lint & webpack --env production",
    "lint": "eslint \"src/**/*.js\" --fix"
  },
  "keywords": [
    "utils",
    "javascript-utils"
  ],
  "author": "molvqingtai",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "live-server": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-build-notifier": "^2.1.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "xlsx": "^0.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molvqingtai/javascript-utils.git"
  },
  "bugs": {
    "url": "https://github.com/molvqingtai/javascript-utils/issues"
  },
  "homepage": "https://github.com/molvqingtai/javascript-utils#readme"
}
